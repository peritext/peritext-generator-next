{
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    "transform-decorators-legacy",
    "transform-decorators",
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-class-properties",
    ["transform-es2015-for-of", {
      "loose": true
    }]
  ],
  "presets": [
    "next/babel",
    "es2015",
    "stage-2",
    "react"
  ]
}